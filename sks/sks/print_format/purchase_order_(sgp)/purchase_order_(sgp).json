 "css": ".master{\n    font-family:arial;\n    font-size:15px;\n}\n\n.master td{\n    font-family:arial;\n    font-size:15px;\n    color: black;\n}\n\n.tax td{\n    border-right:1px solid grey;\n}",
 "custom_format": 1,
 "default_print_language": "en-GB",
 "disabled": 0,
 "doc_type": "Purchase Order",
