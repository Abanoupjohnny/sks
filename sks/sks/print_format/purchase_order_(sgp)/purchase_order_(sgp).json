 "doctype": "Print Format",
 "font": "Default",
 "html": "<div>\n{% if letter_head and not no_letterhead -%}\n{% if print_settings.repeat_header_footer %}\n<div class=\"letter-head\">{{ letter_head }}</div>\n{%- endif %}\n{%- endif %}\n\n<div class=\"master\" style=\"border: 1px solid grey;\">\n    <div style=\"width;100%; border-bottom: 1px solid grey; background-color: orange; font-size:20px; height:70px; border-top:23px solid orange;\n    padding-left: 7px\"><b>PURCHASE ORDER</b></div>\n    <div width=\"100%\">\n        <div style=\"float:left;\" align=\"left\" width=\"70%\">\n        </div>\n        <div align=\"left\" width=\"20%\" style=\"padding-right:15px; float:right;\">\n        PO NO. : <br>\n        Date : {{doc.get_formatted(\"transaction_date\")}}<br>\n        </div>\n    </div>\n    <div width=\"100%\">\n        <table width=\"100%\">\n            <tr>\n                <td><b>Supplier : </b></td>\n                <td><b>{{ doc.supplier }}</b></td>\n            </tr>\n            <tr>\n                {% set supplier = frappe.get_doc(\"Supplier\", doc.supplier)%}\n                <td></td>\n                <td>{{ doc.address_display }}</td>\n            </tr>\n        </table>\n    </div>\n\n        <div style=\"padding:5px;\">\n        {% set serial_no=[] %}\n        <table style=\"border:1px solid grey;\">\n            {% set tablelength=10 %}\n            <tr style=\"border-bottom:1px solid grey\">\n                <td style=\"background-color:orange !important;\"><b>S.No.</b></td>\n                <td style=\"border-left:1px solid grey;background-color:orange !important;\" align=\"center\"><b>Particulars</b></td>\n                <td style=\"border-left:1px solid grey;background-color:orange !important;\" align=\"center\"><b>Qty</b></td>\n               <td style=\"border-left:1px solid grey;background-color:orange  !important;\" align=\"center\"><b>Uom</b></td>\n                <td style=\"border-left:1px solid grey;background-color:orange  !important;\" align=\"center\"><b>Rate</b></td>\n                <td style=\"border-left:1px solid grey;background-color:orange  !important;\" align=\"center\"><b>Amount</b></td>\n            </tr>\n\n            \n            {% for item in doc.items %}\n                {% set _=serial_no.append(1) %}\n            <tr>\n                <td align=\"left\" width=\"6%\">{{ serial_no|length }}</td>\n                <td align=\"left\" style=\"border-left:1px solid grey;\" width=\"36%\">{{ item.item_name }}</td>\n                <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"10%\">{{ item.qty}}</td>\n                <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"6%\">{{ item.uom }}</td>\n                <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"15%\">{{ \"%.2f\"|format(item.rate) }}</td>\n                <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"15%\">{{ \"%.2f\"|format(item.amount) }}</td>\n            </tr>\n            {% endfor %}\n            </table>\n            <table width=\"100%\">\n                <tr>\n                    <td width=\"60%\" align=\"left\"><b>Total Qty : </b>{{ doc.total_qty }}</td>\n                    <td width=\"40%\" align=\"left\"><b>Total Amount : </b>{{ \"%.2f\"|format(doc.total) }}</td>\n                </tr>\n            </table>\n            </div>\n            <!--{% set sgstt = [] %}-->\n            <!--{% set cgstt = [] %}-->\n            <!--{% for gst in doc.ts_tax_breakup_table %}-->\n            <!--{% set _ = sgstt.append(gst.ts_state_amount) %}-->\n            <!--{% set _ = cgstt.append(gst.ts_central_amount) %}-->\n            <!--{% endfor %}-->\n            <div class=\"tax\" width=\"100%\" style=\"padding-right:15px;\">\n                <table style=\"border:1px solid grey; float:right; width:50%;\">\n                    <tr style=\"border-bottom:1px solid grey; background-color:orange\">\n                        <td><b>SNO.</b></td>\n                        <td>GST%</td>\n                        <td>Rate</td>\n                        <td>Amount</td>\n                    </tr>\n                    {% if doc.taxes|length == 2 %}\n                    <tr style=\"border:1px solid grey;\">\n                        <td><b>1</b></td>\n                        <td>CGST</td>\n                        <td>{{doc.taxes[0].rate}}     </td>\n                        <td>{{doc.taxes[0].tax_amount}}</td>\n                    </tr>\n                    <tr style=\"border:1px solid grey;\">\n                        <td><b>2</b></td>\n                        <td>SGST</td>\n                        <td>{{doc.taxes[1].rate}}</td>\n                        <td>{{doc.taxes[1].tax_amount}}</td>\n                    </tr>\n                    {% elif doc.taxes|length == 1 %}\n                    <tr style=\"border:1px solid grey;\">\n                        <td><b>1</b></td>\n                        <td>IGST</td>\n                        <td>{{doc.taxes[0].rate}}     </td>\n                        <td>{{doc.taxes[0].tax_amount}}</td>\n                    </tr>\n                    {% else %}\n                    {% endif %}\n                </table>\n            </div>\n            <!--{{doc.other_charges_calculation}}-->\n    <div>\n        <table width=100%>\n            <tr>\n                <td width=\"60%\"></td>\n                <td width=\"40%\"><b>Subtotal : </b>\n                {% if doc.grand_total %}\n                {{ \"%.2f\"|format(doc.grand_total) }}\n                {%endif%}\n                \n                \n                </td>\n            </tr>\n            <tr>\n                <td width=\"60%\"></td>\n                <td width=\"40%\"><b>Roundoff : </b>\n                {% if doc.rounding_adjustment %}\n                {{ doc.rounding_adjustment }}\n                {%endif%}\n                </td>\n            </tr>\n            <tr>\n                <td width=\"60%\"></td>\n                <td width=\"40%\"><b>Grand Total : </b>\n                {% if doc.rounded_total %}\n                {{ \"%.2f\"|format(doc.rounded_total) }}\n                {%endif%}\n                \n                </td>\n            </tr>\n            </table>\n            <table>\n            <tr>\n                <td width=\"100%\"><b>Amount in Words : </b>{{ doc.in_words }}</td>\n                <td width=\"0%\"><b></b></td>\n            </tr>\n            <tr>\n               \n                <td width=\"100%\"><b>Terms : </b>{{ doc.terms }}</td>\n                <td width=\"0%\"></td>\n            </tr>\n            </table>\n        </div>\n            <div width=\"100%\">\n            <table width=\"100%\" style=\"padding: 5px\">\n            <tr>\n                <td width=\"60%\"></td>\n                <td width=\"40%\">For SRI KUMARAN SUPERSTORE</td>\n            </tr>\n            <tr>\n                <td width=\"60%\"></td>\n                <td width=\"40%\"><br><br><br></td>\n            </tr>\n            </table>\n            </div>\n    \n    </div>\n</div>",
