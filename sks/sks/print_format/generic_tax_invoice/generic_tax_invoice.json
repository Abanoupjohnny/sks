 "css": "\t.print-format table, .print-format tr, \n\t.print-format td, .print-format div, .print-format p {\n\t\tline-height: 150%;\n\t\tvertical-align: middle;\n\t}\n\t@media screen {\n\t\t.print-format {\n\t\t\twidth: 4in;\n\t\t\tpadding: 0.25in;\n\t\t\tmin-height: 8in;\n\t\t}\n\t}\n\t\n\t.main table{\n\t    font-size:13px;\n\t}\n\t\n\t.namedate td{\n\t    padding: 0!important; border-spacing: 0!important;  margin:0!important;\n\t}\n\t\n\t.table td{\n\t    padding: 0!important; border-spacing: 0!important;  margin:0!important;\n\t}\n\n\t\n\t.discountleft{\n\t    float:left;\n\t    width:50%;\n\t    \n\t}\n\t\n\t.discountright{\n\t    float:right;\n\t    width:50%;\n\t}\n\t\n\t.discount{\n\t    width:100%;\n\t}\n\t\n\t.discountright td{\n\t    padding: 0!important; border-spacing: 0!important;  margin:0!important;\n\t}\n\t\n\t.tax{\n\t    width:100%;\n\t}",
