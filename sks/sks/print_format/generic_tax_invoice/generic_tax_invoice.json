 "html": "{% set serial_no=[] %}\n<div class=\"main\">\n    <div>\n        <h3 align=\"center\">Invoice</h3>\n        <!--<p align=\"center\">9333911911</p>-->\n        <p align=\"center\">Tax Invoice</p>\n    </div>\n    <div class=\"namedate\">\n        <table width=\"100%\">\n            <tr>\n                <td align=\"left\">{{ doc.customer }}</td>\n                <td align=\"right\">Date: {{ doc.posting_date }}</td>\n            </tr>\n            <tr>\n                <td></td>\n                <td align=\"right\">Invoice No: {{ doc.name }}</td>\n            </tr>\n        </table>\n    </div>\n    <div class=\"table\">\n        <table width=\"100%\">\n            <tr style=\"border-bottom:1px solid black;\">\n                <td align=\"left\">SR</td>\n                <td align=\"left\">Name</td>\n                <td align=\"left\">Qty</td>\n                <td align=\"right\">Price</td>\n                <td align=\"right\">Amount</td>\n            </tr>\n            <tr>\n                {% for item in doc.items %}\n                {% set _=serial_no.append(1) %}\n                <td align=\"left\">{{ serial_no|length }}</td>\n                <td align=\"left\">{{ item.item_name }}<br>HSN: {{ item.gst_hsn_code }}</td>\n                <td align=\"right\">{{ item.qty}}</td>\n                <td align=\"right\">{{\"%.2f\"|format(item.rate)}}</td>\n                <td align=\"right\">{{\"%.2f\"|format(item.amount)}}<br>Discount( {{ item.discount_percentage }} ): {{\"%.2f\"|format(item.discount_amount)}}<br>GST: </td>\n            </tr>\n                {% endfor %}\n            <tr style=\"border-top:1px solid black;\">\n                <td align=\"left\">Total</td>\n                <td></td>\n                <td align=\"right\">{{\"%.2f\"|format(doc.total_qty)}}</td>\n                <td></td>\n                <td align=\"right\">{{\"%.2f\"|format(doc.grand_total)}}</td>\n            </tr>\n        </table>\n    </div>\n    <div class=\"discount\">\n        <div class=\"discountleft\">\n            <table width=\"100%\">\n                <tr>\n                    <td>&nbsp</td>\n                    <td>&nbsp</td>\n                </tr>\n                <tr>\n                    <td>&nbsp</td>\n                    <td>&nbsp</td>\n                </tr>\n                <tr>\n                    <td>&nbsp</td>\n                    <td>&nbsp</td>\n                </tr>\n                <tr>\n                    <td>&nbsp</td>\n                    <td>&nbsp</td>\n                </tr>\n                <tr>\n                    <td>&nbsp</td>\n                    <td>&nbsp</td>\n                </tr>\n            </table>\n        </div>\n        <div class=\"discountright\">\n            <table width=\"100%\">\n                <tr>\n                    <td align=\"right\">Discount({{doc.additional_discount_percentage}}):</td>\n                    <td align=\"right\">{{\"%.2f\"|format(doc.discount_amount)}}</td>\n                </tr>\n                <tr>\n                    <td align=\"right\">Tax:</td>\n                    <td align=\"right\">{{\"%.2f\"|format(doc.total_taxes_and_charges)}}</td>\n                </tr>\n                <tr>\n                    <td align=\"right\">Total:</td>\n                    <td align=\"right\">{{\"%.2f\"|format(doc.grand_total)}}</td>\n                </tr>\n                <tr>\n                    <td align=\"right\">Received:</td>\n                    <td align=\"right\">{{\"%.2f\"|format(doc.paid_amount)}}</td>\n                </tr>\n                <tr>\n                    <td align=\"right\">Balance:</td>\n                    <td align=\"right\">{{\"%.2f\"|format(doc.outstanding_amount)}}</td>\n                </tr>\n            </table>\n        </div>\n        <div class=\"tax\">\n             <p align=\"left\">Tax Details</p>\n            <div class=\"discountleft\">\n                <!--<table width=\"100%\">-->\n                <!--    <tr>-->\n                <!--        <td>&nbsp</td>-->\n                <!--        <td>&nbsp</td>-->\n                <!--    </tr>-->\n                <!--    <tr>-->\n                <!--        <td>&nbsp</td>-->\n                <!--        <td>&nbsp</td>-->\n                <!--    </tr>-->\n                <!--    <tr>-->\n                <!--        <td>&nbsp</td>-->\n                <!--        <td>&nbsp</td>-->\n                <!--    </tr>-->\n                <!--</table>-->\n            </div>\n            <div class=\"discountright\">\n               \n                <table width=\"100%\">\n                    {% if doc.taxes|length == 2 %}\n                    <tr>\n                        <td align=\"right\">SGST:</td>\n                        <td align=\"right\">{{doc.taxes[1].tax_amount}}</td>\n                    </tr>\n                    <tr>\n                        <td align=\"right\">CGST</td>\n                        <td align=\"right\">{{doc.taxes[0].tax_amount}}</td>\n                    </tr>\n                    \n                      {% elif doc.taxes|length == 1 %}\n\n                    <tr>\n                        <td align=\"right\">IGST</td>\n                        <td align=\"right\">doc.taxes[0].tax_amount</td>\n                    </tr>\n                    {% else %}\n                    {% endif %}\n                    \n                </table>\n            </div>\n        </div>\n    </div>\n</div>",
